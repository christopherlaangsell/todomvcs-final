{:lein         {:profile "+shadow-cljs"}
 :builds       {:app {:target     :browser
                      :output-dir "resources/public/js"
                      :asset-path "/js"
                      :modules    {:app {:entries [reagent-example.core]}}
                      :devtools   {:after-load reagent-example.core/mount-root}}}
 :dev-http     {3000 {:root    "resources/public"
                      :handler reagent-example.handler/app}}}
